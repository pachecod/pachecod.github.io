    "room1": {
      "name": "Room 1",
      "image": "https://hkfsgnludtapbbtsacwd.supabase.co/storage/v1/object/public/files/Noah%20Amaral/images/IMG-20250915-163710-125_1757969510140.jpg",
      "hotspots": [
        {
          "id": 1,
          "type": "text",
          "position": "5.23 1.22 5.36",
          "text": "The S.I. Newhouse School of Public Communications is a highly regarded communication school at Syracuse University, known for its cutting-edge facilities and professional programs in advertising, broadcast and digital journalism, public relations, and other fields. Founded in 1934, it is one of the oldest journalism programs in the nation and offers both undergraduate and graduate degrees, preparing ethical communicators for careers in the media.",
          "scene": "room1",
          "navigationTarget": null,
          "audio": null,
          "popupWidth": 7,
          "popupHeight": 2
        },
        {
          "id": 2,
          "type": "text",
          "position": "-6.09 5.95 -0.41",
          "text": "Crouse College, a historic building that houses the College of Visual and Performing Arts (VPA) and the Setnor School of Music. It was built in the late 1880s as a memorial to John Crouse's wife and is known for its Romanesque Revival architecture, including a bell tower, stained glass windows, and the Setnor Auditorium with its 3,823-pipe Holtkamp Organ.",
          "scene": "room1",
          "navigationTarget": null,
          "audio": null,
          "popupWidth": 6.4,
          "popupHeight": 1.9
        },
        {
          "id": 3,
          "type": "navigation",
